{
    "name": "Wordler",
    "description": "Allows usage of Wordler engines on the site",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://www.powerlanguage.co.uk/wordle/"],
            "css": ["dist/wordler.css"],
            "js": ["dist/content.js"]
        }
    ],
    "background": {
        "service_worker": "dist/background.js"
    }
}